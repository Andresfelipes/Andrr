@model Proyecto.Models.Tb_Dian

@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<header class="page-header">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12">
                <h1 class="page-header-heading"><span class="typcn typcn-clipboard page-header-heading-icon"></span>DIANZ <small>REGISTRAR</small></h1>

            </div>
        </div>
    </div>
</header>
<br />


@using (Html.BeginForm())
{

    @Html.AntiForgeryToken()



    <div class="container-fluid ">
    <div class="col-md-12">
        <div class="widget widget-default">
           
                        <div class="widget-header">Registrar</div>
          <div  class="widget-body">
               <div class="form">

                 
                    
                    

            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Código</label>

                @Html.EditorFor(model => model.Codigo, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Codigo, "", new { @class = "text-danger" })

            </div>

            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Resolución</label>

                @Html.EditorFor(model => model.Resolucion, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Resolucion, "", new { @class = "text-danger" })

            </div>

            <div class="form-group col-md-6">

                <label id="lblid" class="control-label labels">Fecha Inicio</label>

                @Html.EditorFor(model => model.Fecha_inicio_vigencia, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Fecha_inicio_vigencia, "", new { @class = "text-danger" })

            </div>

            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Fecha Fin</label>

                @Html.EditorFor(model => model.Fecha_fin_vigencia, new { htmlAttributes = new { @class = "form-control" } })

            </div>

            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Número Inicio</label>

                @Html.EditorFor(model => model.Num_inicio_rango, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Num_inicio_rango, "", new { @class = "text-danger" })

            </div>

            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Número Fin</label>

                @Html.EditorFor(model => model.Num_fin_rango, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Num_fin_rango, "", new { @class = "text-danger" })
            </div>


            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Número Actual</label>

                @Html.EditorFor(model => model.Num_actual, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Num_actual, "", new { @class = "text-danger" })
            </div>


            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Sucursal</label>

                @Html.DropDownList("Sucursal", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Sucursal, "", new { @class = "text-danger" })

            </div>

            <div class="form-group col-md-6">
                <label id="lblid" class="control-label labels">Estado</label>

                @Html.EditorFor(model => model.Estado, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.Estado, "", new { @class = "text-danger" })

            </div>
                   <div class="form-group">
                       <div class="col-md-offset-5 col-md-10">
                           <input type="submit" value="REGISTRAR" class="btn btn-primary" />
                       </div>
                   </div>
</div>
              </div>
            </div>
        </div>
        </div>
        

            
            }





